endef
TARGET_DEVICES += hc5661a

define Device/hc5661a-nand
  BOARDNAME := HC5661A_NAND
  BOARD_NAME := hc5661a
  IMAGE_SIZE := 133169152
  DEVICE_TITLE := HiWiFi HC5661A with SPI NAND
  MTDPARTS := spi0.1:130944k(ubi)
  BLOCKSIZE := 128KiB
  PAGESIZE := 2048
  UBINIZE_OPTS := -E 5
  KERNEL_IN_UBI := 1
  IMAGE/ubi.bin := append-ubi | check-size $$$$(IMAGE_SIZE)
  IMAGE/sysupgrade.bin := sysupgrade-tar | append-metadata | check-size $$$$(IMAGE_SIZE)
  IMAGES += ubi.bin
  SUPPORTED_DEVICES := hc5661a
  DEVICE_PACKAGES := $(PANDORABOX_ROUTER_NAS_LITE) $(MMC_SUPPORT) $(MTKWIFI_MT7628_SUPPORT)
endef
TARGET_DEVICES += hc5661a-nand

