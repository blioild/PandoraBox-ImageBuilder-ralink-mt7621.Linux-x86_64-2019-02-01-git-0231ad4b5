define Device/xiaomi-r3
  BOARDNAME := XIAOMI_R3
  BLOCKSIZE := 128KiB
  PAGESIZE := 2048
  FILESYSTEMS := squashfs
  IMAGE_SIZE := 33554432
  UBINIZE_OPTS := -E 5
  KERNEL_SIZE := 2048k
  KERNEL := kernel-bin | patch-cmdline | lzma | uImage lzma
#   IMAGES := sysupgrade.tar kernel.bin rootfs.bin sysupgrade.bin
#   IMAGE/sysupgrade.tar := sysupgrade-tar | append-metadata
#   IMAGE/kernel.bin := append-kernel
#   IMAGE/rootfs.bin := append-ubi | check-size $$$$(IMAGE_SIZE)
  IMAGE/sysupgrade.bin := append-kernel | pad-to $$$$(KERNEL_SIZE) | append-ubi
  DEVICE_TITLE :=Xiaomi R3
  DEVICE_PACKAGES := $(PANDORABOX_ROUTER_NAS_LITE) $(USB2_SUPPORT) $(MTKWIFI_RT2860V2_SUPPORT) $(MTKWIFI_MT76X2E_SUPPORT)
endef
TARGET_DEVICES += xiaomi-r3

